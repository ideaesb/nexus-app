		<t:checkbox t:id="sustainability" t:value="entity.sustainability"  class="element checkbox" /><t:label for="sustainability" class="choice" />
		<t:checkbox t:id="resilience" t:value="entity.resilience"  class="element checkbox" /><t:label for="resilience" class="choice" />
		<t:checkbox t:id="water" t:value="entity.water"  class="element checkbox" /><t:label for="water" class="choice" />
		<t:checkbox t:id="extremes" t:value="entity.extremes"  class="element checkbox" /><t:label for="extremes" class="choice" />
		<t:checkbox t:id="conservation" t:value="entity.conservation"  class="element checkbox" /><t:label for="conservation" class="choice" />
	

       </div>
       
      </fieldset>
     </li>



