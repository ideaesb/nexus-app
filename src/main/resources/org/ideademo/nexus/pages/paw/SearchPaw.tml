		<t:checkbox t:id="connecticut" t:value="entity.connecticut"  class="element checkbox indent2" /><t:label for="connecticut" class="indent2" />
		<t:checkbox t:id="rhodeIsland" t:value="entity.rhodeIsland"  class="element checkbox indent2" /><t:label for="rhodeIsland" class="indent2" />
		<t:checkbox t:id="midAtlantic" t:value="entity.midAtlantic"  class="element checkbox indent" /><t:label for="midAtlantic" class="indent" />
		<t:checkbox t:id="newYork" t:value="entity.newYork"  class="element checkbox indent2" /><t:label for="newYork" class="indent2" />
		<t:checkbox t:id="newJersey" t:value="entity.newJersey"  class="element checkbox indent2" /><t:label for="newJersey" class="indent2" />
		<t:checkbox t:id="pennsylvania" t:value="entity.pennsylvania"  class="element checkbox indent2" /><t:label for="pennsylvania" class="indent2" />
		<t:checkbox t:id="marlyland" t:value="entity.marlyland"  class="element checkbox indent2" /><t:label for="marlyland" class="indent2" />
