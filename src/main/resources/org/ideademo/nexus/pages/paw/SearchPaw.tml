     <li class="buttons">
       <t:submit t:id="search" value="Search" />
       (<t:pagelink page="prop:componentResources.pageName">CLEAR - form</t:pagelink>)
       (<t:pagelink page="paws">CANCEL - go to list</t:pagelink>)
       
