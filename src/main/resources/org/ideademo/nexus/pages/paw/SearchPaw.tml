		<t:checkbox t:id="northCarolina" t:value="entity.northCarolina"  class="element checkbox indent2" /><t:label for="northCarolina" class="indent2" />
		<t:checkbox t:id="southCarolina" t:value="entity.southCarolina"  class="element checkbox indent2" /><t:label for="southCarolina" class="indent2" />
		<t:checkbox t:id="localCity" t:value="entity.localCity"  class="element checkbox" /><t:label for="localCity" class="choice" />
		<t:checkbox t:id="problemFocused" t:value="entity.problemFocused"  class="element checkbox" /><t:label for="problemFocused" class="choice" />


       </div>
       
      </fieldset>
     </li>




