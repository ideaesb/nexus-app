<html t:type="longform" title="Paws Worksheet"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
      
<body>
   <t:form t:id="paw" class="appnitro">
   
   <div class="form_description">
     <h2>Eastern Region RCSD Projects &amp; Activities (PAWS) Worksheet</h2>
     <p>Use this form to add or update a PAWS record into EPiko (Worsheet Version April 27 2012)</p>
   </div>		
   
   <t:errors/>

   <ul>
   
     <li id="li_code">
      <t:label class="description" for="code" />
      <t:textField t:id="code" t:validate="maxlength=255" size="15" value="entity.code" />
     </li>
     
     <li id="li_name">
      <t:label class="description" for="name" />
      <div>
       <t:textArea t:id="name" t:validate="maxlength=255" rows="3" cols="90" value="entity.name" />
      </div>
     </li>

     <li id="li_agency">
      <t:label class="description" for="organization" />
      <div>
       <t:textArea t:id="organization" t:validate="maxlength=255" rows="3" cols="90" value="entity.organization" />
      </div>
     </li>

     <li id="li_contact">
      <t:label class="description" for="contact" />
      <div>
       <t:textArea t:id="contact" t:validate="maxlength=255" rows="2" cols="90" value="entity.contact" />
      </div>
     </li>
    	<li id="li_url">
	<t:label class="description" for="url" />
	<t:textArea t:id="url" t:validate="maxlength=255" rows="3" cols="90" value="entity.url" />
	</li>
	<li id="li_description">
	<t:label class="description" for="description" />
	<t:textArea t:id="description" t:validate="maxlength=4096" rows="5" cols="90" value="entity.description" />
	</li>
	<li id="li_keywords">
	<t:label class="description" for="keywords" />
	<t:textArea t:id="keywords" t:validate="maxlength=4096" rows="5" cols="90" value="entity.keywords" />
	</li>
	<li id="li_objectives">
	<t:label class="description" for="objectives" />
	<t:textArea t:id="objectives" t:validate="maxlength=255" rows="3" cols="90" value="entity.objectives" />
	</li>
	<li id="li_worksheet">
	<t:label class="description" for="worksheet" />
	<t:textArea t:id="worksheet" t:validate="maxlength=255" rows="3" cols="90" value="entity.worksheet" />
	</li>
	<li id="li_dates">
	<t:label class="description" for="dates" />
	<t:textArea t:id="dates" t:validate="maxlength=255" rows="3" cols="90" value="entity.dates" />
	</li>
	<li id="li_resources">
	<t:label class="description" for="resources" />
	<t:textArea t:id="resources" t:validate="maxlength=255" rows="3" cols="90" value="entity.resources" />
	</li>
	<li id="li_feedback">
	<t:label class="description" for="feedback" />
	<t:textArea t:id="feedback" t:validate="maxlength=255" rows="3" cols="90" value="entity.feedback" />
	</li>
		
     
     
     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('statusBox');" title="Show or Hide">Status</legend>
       <div id="statusBox" class="unhidden">
        <t:checkbox t:id="ongoing" t:value="entity.ongoing" class="element checkbox" /><t:label for="ongoing" class="choice"/>
        <t:checkbox t:id="planned" t:value="entity.planned" class="element checkbox" /><t:label for="planned" class="choice"/>
        <t:checkbox t:id="proposed" t:value="entity.proposed" class="element checkbox" /><t:label for="proposed" class="choice"/>
        <t:checkbox t:id="completed" t:value="entity.completed" class="element checkbox" /><t:label for="completed" class="choice" />
       </div>
       
      </fieldset>
     </li>


     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('priorityBox');" title="Show or Hide">Priority</legend>
       <div id="priorityBox" class="unhidden">
		<t:checkbox t:id="high" t:value="entity.high"  class="element checkbox" /><t:label for="high" class="choice" />
		<t:checkbox t:id="mid" t:value="entity.mid"  class="element checkbox" /><t:label for="mid" class="choice" />
		<t:checkbox t:id="low" t:value="entity.low"  class="element checkbox" /><t:label for="low" class="choice" />
		<t:checkbox t:id="unknown" t:value="entity.unknown"  class="element checkbox" /><t:label for="unknown" class="choice" />

       </div>
       
      </fieldset>
     </li>


     <li>     
