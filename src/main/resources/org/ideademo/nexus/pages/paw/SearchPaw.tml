	    <t:checkbox t:id="retrofitted" t:value="entity.retrofitted" class="element checkbox" /><t:label for="retrofitted" class="choice" />
		<t:checkbox t:id="monitoring" t:value="entity.monitoring" class="element checkbox" /><t:label for="monitoring"  class="choice" />
		<t:checkbox t:id="research" t:value="entity.research" class="element checkbox" /><t:label for="research" class="choice" />

       </div>
       
      </fieldset>
     </li>



